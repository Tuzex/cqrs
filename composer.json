{
  "name": "tuzex/cqrs",
  "description": "A set of abstractions for CQRS (Command Query Responsibility Segregation)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "autoload": {
    "psr-4": {
      "Tuzex\\Cqrs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tuzex\\Cqrs\\Test\\": "tests"
    }
  },
  "require": {
    "php": "^7.2|8.*"
  },
  "require-dev": {
    "symplify/easy-coding-standard": "10.0.*"
  }
}
